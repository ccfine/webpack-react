{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf dist",
    "build:client": "webpack --config ./config/webpack.client.config.js",
    "build:server": "webpack --config ./config/webpack.server.config.js",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.client.config.js",
    "server": "nodemon ./server/server.js"
  },
  "repository": {
    "type": "git",
